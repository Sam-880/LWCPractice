<aura:component >
    <aura:attribute name="empName" type="String" />
    <aura:attribute name="salary" type="Decimal" />
    <lightning:button label="SetData" onclick="{!c.show}" />
    <lightning:button label="Cancel" onclick="{!c.invoke}" />
    <div>
        Name : {!v.empName}<br/><br/>
        Salary : {!v.salary}<br/><br/>
    </div>
</aura:component>